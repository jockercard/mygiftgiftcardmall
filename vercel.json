{
  "version": 2,
  "buildCommand": null,
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    { "source": "/sitemap.xml", "destination": "/api/sitemap" },
    { "source": "/faqs", "destination": "/faqs/index.html" },
    { "source": "/how-it-works", "destination": "/how-it-works/index.html" },
    { "source": "/contact", "destination": "/contact/index.html" },
    { "source": "/check-balance", "destination": "/check-balance/index.html" },
    { "source": "/faq", "destination": "/faqs/index.html" },
    { "source": "/learnmore", "destination": "/how-it-works/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    },
    {
      "source": "/sitemap.xml",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache, no-store, must-revalidate" }
      ]
    }
  ]
}
